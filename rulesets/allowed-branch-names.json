{
  "name": "Allowed branch names",
  "target": "branch",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "include": ["refs/heads/*"],
      "exclude": []
    }
  },
  "rules": [
    {
      "type": "branch_name_pattern",
      "parameters": {
        "name": "",
        "pattern": "^(dev|sbx|prd|feature\\/LI9INFRA-.*)$",
        "operator": "regex",
        "negate": false
      }
    }
  ]
}
